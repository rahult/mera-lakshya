<h1>Listing scores</h1>

<table>
  <tr>
    <th>Club</th>
    <th>User</th>
    <th>Date</th>
    <th>Range</th>
    <th>Weather</th>
    <th>Score</th>
    <th>Comments</th>
    <th>Target photo uid</th>
    <th>Target photo name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @scores.each do |score| %>
  <tr>
    <td><%= score.club_id %></td>
    <td><%= score.user_id %></td>
    <td><%= score.date %></td>
    <td><%= score.range %></td>
    <td><%= score.weather %></td>
    <td><%= score.score %></td>
    <td><%= score.comments %></td>
    <td><%= score.target_photo_uid %></td>
    <td><%= score.target_photo_name %></td>
    <td><%= link_to 'Show', score %></td>
    <td><%= link_to 'Edit', edit_score_path(score) %></td>
    <td><%= link_to 'Destroy', score, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Score', new_score_path %>
